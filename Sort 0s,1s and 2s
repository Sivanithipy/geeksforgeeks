class Solution {
    public void sort012(int[] arr) {
        // code here
        HashMap<Integer,Integer> map=new HashMap<>();
        ArrayList<Integer> array=new ArrayList<>();
        for(int num:arr)
        {
            map.put(num,map.getOrDefault(num,0)+1);
        }
        int index=0;
        for(int i=0;i<=2;i++)
        {
            int count=map.getOrDefault(i,0);
            while(count-- >0)        
            arr[index++]=i;
        }
    }
}
