// User function Template for Java

class Solution {
    static int evenlyDivides(int n) {
        // code here
        int count=0,add=n;
        while(n>0)
        {
            int sum=n%10;
            if(sum!=0 && add%sum==0)
            {
                count++;
            }
            n=n/10;
        }
        return count;
    }
}
