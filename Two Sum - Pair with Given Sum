class Solution {
    boolean twoSum(int arr[], int target) {
        // code here
        int n = arr.length;
        if(n == 1) return false;
        for(int i = 0;i < n;i++)
        {
            for(int j = i+1;j < n;j++)
            {
                if(target == arr[i]+arr[j])
                {
                    return true;
                }
            }
        }
        return false;
    }
}
