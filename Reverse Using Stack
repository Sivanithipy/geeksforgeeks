class Solution {
    public String reverse(String s) {
        // code here
        Stack <Character> st = new Stack<>();
        
        for(char c : s.toCharArray())
        {
            st.push(c);
        }
        StringBuilder res = new StringBuilder();
        while(!st.isEmpty())
        {
            res.append(st.pop());
        }
        
        return res.toString();
    }
}
