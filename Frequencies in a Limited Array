class Solution {
    public List<Integer> frequencyCount(int[] arr) {
        // code here
        int n=arr.length;
        List <Integer> list = new ArrayList<>();
        HashMap <Integer,Integer> map = new HashMap<>();
        for(int num:arr)
        {
            map.put(num,map.getOrDefault(num,0)+1);
        }
        for (int i = 1; i <= n; i++) {
            list.add(map.getOrDefault(i, 0));
        }
        return list;
    }
}
