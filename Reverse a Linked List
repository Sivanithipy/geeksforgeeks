/*
class Node {
    int data;
    Node next;
    Node(int value) {
        this.value = value;
    }
}
*/

class Solution {
    Node reverseList(Node head) {
        // code here
        Node prev=null;
        Node curr=head;
        while(curr!=null)
        {
            Node newNode=curr.next;
            curr.next=prev;
            prev=curr;
            curr=newNode;
        }
        return prev;
    }
}
