class Solution {
    public static boolean areAnagrams(String s1, String s2) {
        // code here
       
       int n=s1.length();
       int o=s2.length();
       if(n!=o) return false;
       int[] count = new int[256];
       for(int i=0;i<n;i++)
       {
           count[s1.charAt(i)]++;
           count[s2.charAt(i)]--;
       }
       for(int val:count)
       {
           if(val!=0) return false;
       }
       return true;
    }
}
