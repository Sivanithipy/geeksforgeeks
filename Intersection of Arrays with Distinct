class Solution {
    public static int intersectSize(int a[], int b[]) {
        // Your code here
        int count=0;
        HashMap<Integer,Integer> freq=new HashMap<>();
        for(int num:a)
        {
            freq.put(num,freq.getOrDefault(num,0)+1);
        }
        for(int num:b)
        {
           if(freq.containsKey(num)&&freq.get(num)>0)
           {
               count++;
               freq.put(num,freq.get(num)-1);
           }
        }
        return count;
    }
}
