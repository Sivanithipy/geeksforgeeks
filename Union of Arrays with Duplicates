class Solution {
    public static ArrayList<Integer> findUnion(int[] a, int[] b) {
        // code here
        HashSet<Integer>set=new HashSet<>();
        for(int num:a)
        {
            set.add(num);
        }
        for(int num1:b)
        {
            set.add(num1);
        }
        ArrayList<Integer> add=new ArrayList<>(set);
        Collections.sort(add);
        return add;
    }
}
